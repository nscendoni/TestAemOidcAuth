<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/wintergw2025/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        jcr:title="Internal Server Error"
        sling:resourceType="wintergw2025/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wintergw2025/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wintergw2025/components/container"
                layout="responsiveGrid">
                <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wintergw2025/components/title"
                    jcr:title="500 - Internal Server Error"
                    type="h1"/>
                <text
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wintergw2025/components/text"
                    text="&lt;p&gt;We're sorry, but something went wrong on our end. Our team has been notified and is working to resolve the issue.&lt;/p&gt;&lt;p&gt;Please try again later or contact support if the problem persists.&lt;/p&gt;"
                    textIsRich="true"/>
                <button
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wintergw2025/components/button"
                    jcr:title="Return to Home"
                    linkURL="/content/wintergw2025/us/en.html"
                    linkTarget="_self"/>
            </container>
        </root>
    </jcr:content>
</jcr:root>
